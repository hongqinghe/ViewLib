<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".ViewActivity"
	tools:layout_editor_absoluteY="81dp">


	<Button
		android:id="@+id/to_contrainLayout"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="26dp"
		android:onClick="toContrainLayout"
		android:text="toConstrainlayout"
		app:layout_constraintEnd_toStartOf="@+id/textabView"
		app:layout_constraintStart_toEndOf="@+id/ViewAuto"
		app:layout_constraintTop_toBottomOf="@+id/buttonRecycleView"
		tools:ignore="HardcodedText" />

	<Button
		android:id="@+id/button"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="6dp"
		android:layout_marginTop="28dp"
		android:layout_marginEnd="2dp"
		android:onClick="toListView"
		android:text="listView"
		app:layout_constraintEnd_toStartOf="@+id/buttonRecycleView"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<Button
		android:id="@+id/buttonRecycleView"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="28dp"
		android:onClick="toRecycleView"
		android:text="RecycleView"
		app:layout_constraintEnd_toStartOf="@+id/StatusBarSet"
		app:layout_constraintStart_toEndOf="@+id/button"
		app:layout_constraintTop_toTopOf="parent" />

	<Button
		android:id="@+id/ViewAuto"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="21dp"
		android:layout_marginEnd="17dp"
		android:onClick="viewAuto"
		android:text="viewAuto"
		app:layout_constraintBaseline_toBaselineOf="@+id/to_contrainLayout"
		app:layout_constraintEnd_toStartOf="@+id/to_contrainLayout"
		app:layout_constraintStart_toStartOf="parent"
		tools:ignore="HardcodedText" />

	<Button
		android:id="@+id/StatusBarSet"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="28dp"
		android:layout_marginEnd="8dp"
		android:onClick="statusBar"
		android:text="statusBar"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@+id/buttonRecycleView"
		app:layout_constraintTop_toTopOf="parent"
		tools:ignore="HardcodedText" />

	<Button
		android:id="@+id/StatusBarSettest"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="28dp"
		android:layout_marginEnd="80dp"
		android:onClick="statusTestActivity"
		android:text="statusTestActivity"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textabView"
		tools:ignore="HardcodedText" />

	<Button
		android:id="@+id/textabView"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginEnd="4dp"
		android:onClick="textTabViewActivity"
		android:text="TextTabView"
		app:layout_constraintBaseline_toBaselineOf="@+id/to_contrainLayout"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@+id/to_contrainLayout"
		tools:ignore="HardcodedText" />

	<Button
		android:id="@+id/expandable"
		style="@style/button_style"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginTop="29dp"
		android:onClick="expandableActivity"
		android:text="ExpandableActivity"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/ViewAuto"
		tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/focus"
        style="@style/button_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="44dp"
        android:onClick="focusAc"
        android:text="focusAc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/expandable"
        tools:ignore="HardcodedText,OnClick" />

	<Button
		style="@style/button_style"
		android:id="@+id/menu"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="68dp"
		android:text="menu"
		android:textAllCaps="false"
		android:onClick="toMenu"
		app:layout_constraintBaseline_toBaselineOf="@+id/focus"
		app:layout_constraintStart_toEndOf="@+id/focus" />


</android.support.constraint.ConstraintLayout>
